from app import app


@app.route('/log', methods=['GET'])
def get_log():
    with open('pom-poms.log', 'r') as f:
        return f.read()


@app.route('/')
def return_data_from_flask():
    info = {"id_order": 0}
    return (info), 200


def tmp_test_tmp():
    x = {"ping": "pong", "ping_1": "pong_1"}
    return (x)
